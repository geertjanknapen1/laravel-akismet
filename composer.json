{
    "name": "nickurt/laravel-akismet",
    "description": "Akismet for Laravel 5.8",
    "keywords": ["akismet", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "laravel/framework": "5.8.*",
        "guzzlehttp/guzzle": "6.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.5"
    },
    "autoload": {
        "psr-4": {
            "nickurt\\Akismet\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "nickurt\\Akismet\\ServiceProvider"
            ],
            "aliases": {
                "Akismet": "nickurt\\Akismet\\Facade"
            }
        }
    }
}